{
  "_id": "materiel",
  "name": "materiel",
  "pattern": [
     "^/\\w+[\\-\\w]*\\.html$"
  ],
  "rules": {
   	"name":        "//span[@property='v:name'][1]/text()",
    "description": "//span[@class='caract'][1]/text()",
    "price": "//span[@property='v:price'][1]/text()",
    "brand": "//span[@property='v:brand'][1]/text()",
    "model": "//span[@property='v:name'][1]/text()",
    "thumbnail": "(//img[@rel='v:photo'])[1]/@src"
  }
}
