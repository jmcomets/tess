  "name": "zalando",
  "pattern": [
     "/[\\w\\-]+/[\\w\\-]+.html"
  ],
  "rules": {
    "name":        "//span[@itemprop='name'][1]/text()",
    "description": "//span[@itemprop='description'][1]/text()",
    "price": "(//span[contains(@class, 'price') and not(contains(@class, 'oldPrice'))])[1]",
