{
  "manifest_version": 2,

  "name": "Tess Labeler",
  "description": "Extension to help gather data for Tess",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "debugger",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "notifications"
  ]
}
